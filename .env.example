# AI Configuration
OPENAI_API_KEY=sk-...
DEEPSEEK_API_KEY=sk-...
OPENAI_BASE_URL=https://api.openai.com/v1
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
AI_MODEL=deepseek-chat
AI_TEMPERATURE=0.7

# Database
DATABASE_URL=sqlite:///data/agent.db

# Xianyu Cookie (Get from browser DevTools -> Application -> Cookies)
# Steps: Open goofish.com -> F12 -> Application -> Cookies -> Copy all
XIANYU_COOKIE_1=your_cookie_here
XIANYU_COOKIE_2=your_second_cookie_here

# Encryption (optional - auto-generated if not set)
# ENCRYPTION_KEY=your_secret_passphrase_here

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:8501,http://localhost:3000,http://127.0.0.1:8501

# Rate Limiting
RATE_LIMIT_MAX=30
RATE_LIMIT_WINDOW=60

# App Settings
APP_DEBUG=false
APP_LOG_LEVEL=INFO
