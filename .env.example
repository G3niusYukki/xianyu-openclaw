# === AI Provider (at least one required) ===
# OpenClaw uses this to power skills and conversations.
# Choose one: Anthropic (recommended), OpenAI, DeepSeek, etc.
ANTHROPIC_API_KEY=sk-ant-...
# OPENAI_API_KEY=sk-...
# DEEPSEEK_API_KEY=sk-...

# === OpenClaw Gateway ===
OPENCLAW_GATEWAY_TOKEN=your-secret-token
OPENCLAW_WEB_PORT=8080
AUTH_PASSWORD=changeme
AUTH_USERNAME=admin

# === Xianyu Cookie ===
# Get from browser DevTools -> Network -> Request Headers -> Cookie
# Steps: Open goofish.com -> Login -> F12 -> Network -> Copy Cookie
XIANYU_COOKIE_1=your_cookie_here
XIANYU_COOKIE_2=your_second_cookie_here

# === Cookie Encryption (optional) ===
# Auto-generated if not set
# ENCRYPTION_KEY=your_secret_passphrase

# === AI for content generation (used by Python services) ===
DEEPSEEK_API_KEY=sk-...
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
AI_MODEL=deepseek-chat
AI_TEMPERATURE=0.7

# === Database ===
DATABASE_URL=sqlite:///data/agent.db
